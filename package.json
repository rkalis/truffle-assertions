{
  "name": "truffle-assertions",
  "version": "0.5.0",
  "description": "Additional assertions and utilities for testing Ethereum smart contracts in Truffle unit tests",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test-with-cov": "./node_modules/.bin/nyc --reporter=text-lcov npm test | node_modules/.bin/coveralls"
  },
  "nyc": {
    "temp-directory": "./node_modules/.nyc_output"
  },
  "keywords": [
    "truffle",
    "assert",
    "assertions",
    "test",
    "events",
    "ethereum",
    "solidity"
  ],
  "author": "Rosco Kalis",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rkalis/truffle-assertions"
  },
  "bugs": {
    "url": "https://github.com/rkalis/truffle-assertions/issues"
  },
  "homepage": "https://github.com/rkalis/truffle-assertions",
  "dependencies": {
    "assertion-error": "^1.1.0",
    "web3": "^1.0.0-beta.35"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1"
  }
}
